{
  "name": "@unisonht/lirc",
  "version": "0.0.1",
  "description": "UnisonHT Plugin for LIRC",
  "main": "dist/index.js",
  "types": "src/index.ts",
  "scripts": {
    "test:watch": "nodemon ./dist/test.js",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joeferner/unisonht-lirc.git"
  },
  "keywords": [
    "unisonht",
    "lirc"
  ],
  "author": "Joe Ferner <joe@fernsroth.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joeferner/unisonht-lirc/issues"
  },
  "homepage": "https://github.com/joeferner/unisonht-lirc#readme",
  "dependencies": {
    "debug": "^4.1.1",
    "lirc-client": "^2.0.0"
  },
  "peerDependencies": {
    "@unisonht/unisonht": "0.0.x"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/node": "^12.7.1",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.3"
  }
}
